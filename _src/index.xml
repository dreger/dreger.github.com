---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
<title>kyledreger.com</title>
<link href="{{ site.domain }}/atom.xml" rel="self"/>
<link href="{{ site.domain }}/"/>
<updated>{{ site.time | date_to_xmlschema }}</updated>
<id>{{ site.domain }}/</id>
<!-- <author>
<name>{{ site.author }}</name>
<email>{{ site.email }}</email>
</author> -->
{% for post in site.posts limit:20 %}
{% if post.date %}
<entry>
   <title>{% if post.category == 'log-podcast' %}The Log: {% endif %}{{ post.title }}</title>
   {% if post.category == 'link' %}
   <link href="{{ post.source-link }}"/>
   {% else %}
   <link href="{{site.domain}}{{ post.url }}"/>
   {% endif %}
   <published>{{ post.date | date_to_xmlschema }}</published>
   <id>{{ site.domain }}{{ post.id }}</id>
   <author>
      <name>{{ site.author }}</name>
      <uri>{{ site.domain }}</uri>
   </author>
  <content type="html">
    {{ post.content | xml_escape }}
    &lt;a href=&quot;{{site.domain}}{{ post.url }}&quot;&gt;&#10006;&lt;/a&gt;
  </content>
</entry>
{% endif %}
{% endfor %}
</feed>
