<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd" version="2.0">

<channel>
<title>The Log</title>
<link>http://kyledreger.com/log</link>
<language>en</language>
<copyright>&#xA9; Kyle Dreger</copyright>
<itunes:subtitle>Discussing design, craft, and writing. Recorded from my dorm room.</itunes:subtitle>
<itunes:author>Kyle Dreger</itunes:author>
<itunes:keywords>kyle,dreger,news,craft,design,writing,college,technology</itunes:keywords>
<itunes:summary>A weekly podcast by Kyle Dreger and guests on the topics of design, craft, and all sorts of technology. </itunes:summary>
<description>A weekly podcast by Kyle Dreger and guests on the topics of design, craft, and all sorts of technology. </description>

<itunes:owner>
<itunes:name>Kyle Dreger</itunes:name>
<itunes:email>dregerkq@mountunion.edu</itunes:email>
</itunes:owner>

<itunes:image href="{{site.domain}}/log/assets/itunes.jpg" />
<itunes:category text="Technology" />
<itunes:category text="Podcasting" />

<!-- <itunes:new-feed-url>http://{{ site.domain }}/podcast.xml</itunes:new-feed-url> -->

{% for post in site.categories.log-podcast %}
<item>
<title>{{ post.title }}</title>
<itunes:author>Kyle Dreger</itunes:author>
<itunes:subtitle>The Log: {{ post.title }}</itunes:subtitle>
<itunes:summary>{{ post.content | xml_escape }}</itunes:summary>
<itunes:image href="{{site.domain}}/log/assets/itunes.jpg" />
<enclosure url="{{site.domain}}/log/audio/{{ post.file }}" length="{{ post.length }}" type="audio/mpeg" />
<guid>{{site.domain}}{{ post.url }}</guid>
<pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
<itunes:duration>{{ post.duration }}</itunes:duration>
<itunes:keywords>kyle,dreger,news,craft,design,writing,college,technology</itunes:keywords>
</item>
{% endfor %}
</channel>

</rss>
